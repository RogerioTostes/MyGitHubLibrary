networks:
  mygithublibrarynetwork:
    driver: bridge
services:
  mygithublibrary:
    build:
      context: C:\Users\rtostes\source\repos\MyGitHubLibrary
      dockerfile: MyGitHubLibrary/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MyGitHubLibrary
      target: base
    container_name: MyGitHubLibrary
    depends_on:
    - postgres
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '5001'
      ASPNETCORE_Kestrel__Certificates__Default__Password: password
      ASPNETCORE_Kestrel__Certificates__Default__Path: /https/aspnetapp.pfx
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: mygithublibrary:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/MyGitHubLibrary.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      mygithublibrarynetwork: {}
    ports:
    - published: 5000
      target: 80
    - target: 80
    - published: 5001
      target: 443
    - target: 443
    tty: true
    volumes:
    - C:\Users\rtostes\source\repos\MyGitHubLibrary\MyGitHubLibrary:/app:rw
    - C:\Users\rtostes\.aspnet\https:/https:ro
    - C:\Users\rtostes\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\rtostes\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\rtostes\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\rtostes\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\rtostes\source\repos\MyGitHubLibrary:/src:rw
  pgadmin:
    depends_on:
    - postgres
    environment:
      PGADMIN_DEFAULT_EMAIL: rogertostes@yahoo.com.br
      PGADMIN_DEFAULT_PASSWORD: mygithublibrary
    image: dpage/pgadmin4
    networks:
      mygithublibrarynetwork: {}
    ports:
    - published: 16543
      target: 80
    restart: always
  postgres:
    environment:
      POSTGRES_DB: MyGitHubLibrary
      POSTGRES_PASSWORD: mygithublibrary
      POSTGRES_USER: mygithublibrary
    image: postgres
    networks:
      mygithublibrarynetwork: {}
    ports:
    - published: 15432
      target: 5432
    restart: always
    volumes:
    - C:\Users\rtostes\source\repos\MyGitHubLibrary\Scripts.sql:/docker-entrypoint-initdb.d/init.sql:rw
version: '3.4'